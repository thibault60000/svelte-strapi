		"autoprefixer": "^10.4.14",
		"boxen": "^7.1.1",
		"dayjs": "^1.11.9",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.31.1",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.1",
		"svelte-check": "^3.4.4",
		"svelte-preprocess": "^5.0.4",
		"sveltekit-superforms": "^1.1.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
